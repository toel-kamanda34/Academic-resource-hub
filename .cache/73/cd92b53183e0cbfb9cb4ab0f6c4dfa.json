r(--primary-color);\n    color: var(--secondary-color);\n    padding: 1rem 0;\n    text-align: center;\n}\n\nheader h1 {\n    margin-bottom: 1rem;\n}\n\nheader h1 span {\n    color: var(--gold-color);\n}\n\nnav ul {\n    list-style-type: none;\n}\n\nnav ul li {\n    display: inline;\n    margin-right: 1rem;\n}\n\nnav ul li a {\n    color: var(--secondary-color);\n    text-decoration: none;\n    transition: color 0.3s ease;\n}\n\nnav ul li a:hover {\n    color: var(--gold-color);\n}\n\nmain {\n    padding: 2rem 0;\n}\n\nsection {\n    background-color: var(--secondary-color);\n    margin-bottom: 2rem;\n    padding: 2rem;\n    border-radius: 5px;\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n\nh2 {\n    margin-bottom: 1rem;\n    color: var(--primary-color);\n}\n\n/* Form Styling */\nform {\n    display: flex;\n    flex-direction: column;\n    background-color: var(--secondary-color);\n    padding: 1rem;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n    margin-bottom: 2rem;\n}\n\nlabel {\n    margin-top: 1rem;\n    margin-bottom: 0.5rem;\n    color: var(--primary-color);\n}\n\ninput[type=\"text\"],\ninput[type=\"file\"],\ntextarea,\nselect {\n    width: 100%;\n    padding: 0.5rem;\n    margin-bottom: 1rem;\n    border: 1px solid #ddd;\n    border-radius: 4px;\n}\n\nbutton {\n    background-color: var(--accent-color);\n    color: var(--secondary-color);\n    padding: 0.75rem 1.5rem;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    transition: background-color 0.3s ease;\n    margin-top: 1rem;\n}\n\nbutton:hover {\n    background-color: #6B0000;\n}\n\n/* Resource Cards */\n#resourceContainer,\n#papersContainer {\n    display: grid;\n    gap: 1rem;\n}\n\n.grid-view {\n    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\n}\n\n.list-view {\n    grid-template-columns: 1fr;\n}\n\n.resource-card,\n.paper-card {\n    background-color: var(--secondary-color);\n    border: 1px solid #ddd;\n    border-radius: 5px;\n    padding: 1rem;\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\n    cursor: pointer;\n}\n\n.resource-card:hover,\n.paper-card:hover {\n    transform: translateY(-5px);\n    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\n}\n\n.resource-card h3,\n.paper-card h3 {\n    margin-bottom: 0.5rem;\n    color: var(--primary-color);\n}\n\n.resource-card p,\n.paper-card p {\n    font-size: 0.9rem;\n    color: #666;\n}\n\n.resource-card .tags,\n.paper-card .tags {\n    margin-top: 0.5rem;\n}\n\n.resource-card .tags span,\n.paper-card .tags span {\n    background-color: var(--gold-color);\n    color: var(--primary-color);\n    padding: 0.2rem 0.5rem;\n    border-radius: 3px;\n    font-size: 0.8rem;\n    margin-right: 0.5rem;\n}\n\n/* Footer Styling */\nfooter {\n    background-color: var(--primary-color);\n    color: var(--secondary-color);\n    text-align: center;\n    padding: 1rem;\n    position: fixed;\n    width: 100%;\n    bottom: 0;\n}\n\n/* Clickability improvements */\n.card {\n    cursor: pointer;\n    transition: box-shadow 0.3s ease;\n}\n\n.card:hover {\n    box-shadow: 0 4px 8px rgba(0,0,0,0.1);\n}\n\n.paper-link {\n    display: inline-block;\n    margin-top: 10px;\n    padding: 5px 10px;\n    background-color: #007bff;\n    color: white;\n    text-decoration: none;\n    border-radius: 4px;\n    transition: background-color 0.3s ease;\n}\n\n.paper-link:hover {\n    background-color: #0056b3;\n}\n\n/* Responsive Design */\n@media (max-width: 768px) {\n    nav ul {\n        display: flex;\n        flex-direction: column;\n    }\n\n    nav ul li {\n        margin-bottom: 0.5rem;\n    }\n\n    .grid-view {\n        grid-template-columns: 1fr;\n    }\n\n    form input,\n    form select,\n    form textarea {\n        width: 100%;\n    }\n}\n.success {\n    color: green;\n}\n\n.error {\n    color: red;\n}\n"},"lineCount":null}},"error":null,"hash":"b2cc8f6f7e4de8c5d9b675cd510b5072","cacheData":{"env":{}}}